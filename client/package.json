{
  "name": "@hanzishan/client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chart.js": "^2.9.4",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^6.0.1",
    "react-scripts": "^3.4.1",
    "react-transition-group": "^1.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "ci:test": "CI=true npm run test -- --coverage",
    "ci:build": "CI=true npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.testutil.js",
      "!src/index.js",
      "!src/reducer.js",
      "!src/store.js",
      "!src/serviceWorker.js",
      "!src/i18n/getNavigatorLanguage.js",
      "!src/util/noop.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleNameMapper": {
      "^-!svg-react-loader!(.*)$": "$1"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "fetch-mock": "^7.7.3",
    "jest-fetch-mock": "^2.1.2",
    "jest-localstorage-mock": "^2.4.2",
    "jest-mock-console": "^0.4.2",
    "lodash": "^4.17.21",
    "node-fetch": "^3.1.1",
    "node-sass": "^4.14.1",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "svg-react-loader": "^0.4.6"
  }
}
